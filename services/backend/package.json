{
  "name": "@vteam/backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "DEBUG=* nodemon ./bin/www",
    "test": "NODE_ENV=test DEBUG=backend:* npm exec -- mocha \"./**/*.test.js\" --timeout 10000",
    "coverage": "NODE_ENV=test DEBUG=backend:* c8 --reporter=text --reporter=lcov --report-dir=coverage mocha \"./**/*.test.js\" --timeout 10000",
    "coverageAuth": "NODE_ENV=test DEBUG=backend:* c8 --reporter=text --reporter=lcov --report-dir=coverage mocha ./modules/auth/**/*.test.js --timeout 20000"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.0",
    "morgan": "~1.9.1",
    "openid-client": "^5.7.1",
    "stripe": "^20.1.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "chai": "^6.2.1",
    "mocha": "^11.7.5",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4"
  }
}
